{
  "manifest_version": 3,
  "name": "scrap-ecommerce-data",
  "description": "This chrome extension scraps product details from well-known eCommerce platforms",
  "version": "1.0.0",
  "action" : {
      "default_popup": "popup.html",
      "default_title": "React Boilerplate",
      "default_icon" : "icon.png"
  },
  "icons": {
      "16" : "icon.png",
      "48" : "icon.png",
      "128" : "icon.png"
  }
}